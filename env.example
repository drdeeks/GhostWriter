# Private key for deployment (DO NOT commit real private key!)
PRIVATE_KEY=your_private_key_here

# RPC URLs
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
MODE_RPC_URL=https://mainnet.mode.network
MODE_SEPOLIA_RPC_URL=https://sepolia.mode.network

# Block Explorer API Keys (for contract verification)
BASESCAN_API_KEY=your_basescan_api_key
MODESCAN_API_KEY=your_modescan_api_key

# CoinMarketCap API Key (for gas reporting)
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key

# Gas reporting
REPORT_GAS=false

# Deployed Contract Addresses (fill after deployment)
NFT_CONTRACT_ADDRESS=
STORY_MANAGER_ADDRESS=
LIQUIDITY_POOL_ADDRESS=

# OnchainKit API Key (for frontend)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key

# Chain ID (8453 for Base, 84532 for Base Sepolia, 34443 for Mode, 919 for Mode Sepolia)
NEXT_PUBLIC_CHAIN_ID=84532

# IPFS Base URIs for NFT metadata
NEXT_PUBLIC_HIDDEN_BASE_URI=ipfs://QmHiddenMetadata/
NEXT_PUBLIC_REVEALED_BASE_URI=ipfs://QmRevealedMetadata/
